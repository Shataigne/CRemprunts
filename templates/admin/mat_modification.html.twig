{% extends 'base.html.twig' %}
{% block title %}modification {{categorie}}{% endblock %}
{% block body %}
    {{ form_start(form) }}
    <div class="admin_form">
        {{ form_row(form.libelle) }}
        {{ form_row(form.modele) }}
        {{ form_row(form.marque) }}
        {{ form_row(form.noSerie) }}
        {{ form_row(form.centre) }}
        <div>
            <button type="submit">Modifier</button>
            <a href="{{ path('app_admin_materiel', {'categorie': categorie}) }}"><button>Annuler</button></a>
        </div>
        {{ form_end(form) }}
    </div>
{% endblock %}
